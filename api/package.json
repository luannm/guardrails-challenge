{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "yarn lint && rm -rf ./build && webpack --config webpack.config.js",
    "dev:build": "NODE_ENV=development && yarn build",
    "dev:watch": "NODE_ENV=development webpack --config webpack.config.js --watch",
    "dev:nodemon": "NODE_ENV=development nodemon ./build/server.bundle.js",
    "dev": "yarn dev:build && npm-run-all --parallel dev:nodemon dev:watch",
    "lint": "eslint 'src/**/*.ts' --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^8.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@types/express": "^4.17.3",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-loader": "^8.0.6",
    "eslint": "5.14",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^6.2.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
