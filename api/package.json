{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./build && webpack --config webpack.config.js",
    "dev:build": "NODE_ENV=development && yarn build",
    "dev:watch": "NODE_ENV=development webpack --config webpack.config.js --watch",
    "dev:nodemon": "NODE_ENV=development nodemon ./build/server.bundle.js",
    "dev": "yarn dev:build && npm-run-all --parallel dev:nodemon dev:watch"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "babel-loader": "^8.0.6",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
