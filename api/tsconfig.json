{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es6", "es5", "dom"],
    "allowJs": true,
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "./build"
  },
  "exclude": ["node_modules", "build"]
}
